subr FlowSatInit
;EXEC_begin FlowSatInit FlowSatInit()
# 55 "/opt/PV6.0.1/exp/stan/nmr/lists/pp/FlowSatDef.mod"
 
if(PVM_InFlowSatOnOff == 1)
   {

        40u     modlis4.res
        10u     flowSatGrad.res
   }
 
;EXEC_end   FlowSatInit
# 36 "/opt/PV6.0.1/exp/stan/nmr/lists/pp/MSME.ppg"

